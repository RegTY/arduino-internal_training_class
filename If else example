#include <stdio.h>

int main()
{
    float cgpa;
    printf("Enter your CGPA");
    scanf("%f", &cgpa);
    printf ("%.1f",cgpa);
    if (cgpa >= 3.75){
        printf("Your Grade is an A");
    }
    else if (cgpa >= 3.5){
        printf("Your Grade is an B+");
    }
    else if (cgpa >= 3.0){
        printf("Your Grade is an B-");
    }
    else if (cgpa < 3.0){
        printf("Your Grade is a failure");
    }
}
