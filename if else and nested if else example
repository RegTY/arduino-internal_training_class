#include <stdio.h>

int main()
{
    float cgpa;
    printf("Enter your CGPA:");
    scanf("%.1f", &cgpa);
    if (cgpa >= 2.0) {
        printf("You passed!");
        
    }
    else {
        printf("You failed! Go take a sup paper!");
    }

    return 0;
}



