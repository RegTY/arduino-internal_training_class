*******************************************************************************/
#include <stdio.h>

int main()
{
    float cgpa;
    printf("Enter your CGPA:");
    scanf("%.1f", &cgpa);
    if (cgpa >= 2.0) {
        printf("You passed!");
        
    }
    else {
        printf("You failed! Go take a sup paper!");
    }

    return 0;
}

Example 2 (if Else-if Else)


/******************************************************************************

Welcome to GDB Online.
GDB online is an online compiler and debugger tool for C, C++, Python, PHP, Ruby,
C#, VB, Perl, Swift, Prolog, Javascript, Pascal, HTML, CSS, JS
Code, Compile, Run and Debug online from anywhere in world.

*******************************************************************************/
#include <stdio.h>

int main()
{
    float cgpa;
    printf("Enter your CGPA:");
    scanf("%f", &cgpa);
    if (cgpa > 3.67) {
        printf("Grade : A");
    }
    else if (cgpa >3.33 && cgpa <=3.67){
        printf("Grade : A-");
    }
    else if (cgpa >3 && cgpa <=3.33){
        printf("Grade : B+");

    }
    else if (cgpa >2.67 && cgpa <=3) {
        printf("Grade : B");

    }
    else if (cgpa >2.33 && cgpa <=2.67) {
        printf("Grade : B-");
    }
    else if (cgpa >2 && cgpa <=2.33) {
        printf("Grade : C+");
    }
    else if (cgpa == 2 ) {
        printf("Grade : C");
    }
    else {
        printf("Fail");
    }
    return 0;
}




Example 3 (Nested If-Else)



#include <stdio.h>

int main()
{
    float cgpa;
    char scholar_type[30];
    printf("Enter Scholarship type:");
    scanf("%s", scholar_type);

    printf("Enter your CGPA:");
    scanf("%f", &cgpa);

    if(strcmp(scholar_type ,"A-levels")){
            if(cgpa >= 3.5){
                printf("you are eligible");
            } else{
                printf("You do not qualify :(");
            }

    } else if(strcmp(scholar_type ,"Trust")) {
            if(cgpa >= 3){
                printf("you are eligible");
            } else{
                printf("You do not qualify :(");
            }

    } else {
        printf("Error");
    }

    return 0;
}


